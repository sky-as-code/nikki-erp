meta {
  name: Party Tag - Get by ID
  type: http
  seq: 3
}

get {
  url: {{api_host}}/v1/contacts/parties/tags/{{partytag_id}}
  body: none
  auth: inherit
}

script:post-response {
  const body = JSON.stringify(res.getBody());
  try {
      const payload = JSON.parse(body);
      const etag = payload.etag;
      etag && bru.setVar('etag', etag);
  } catch (err) {
      console.error(err)
  }
  
}

settings {
  encodeUrl: true
}
