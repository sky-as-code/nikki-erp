meta {
  name: User - Get by ID
  type: http
  seq: 3
}

get {
  url: {{api_host}}/v1/identity/users/{{user_id}}?withOrg=true&&withHierarchy=true
  body: none
  auth: inherit
}

params:query {
  withOrg: true
  withHierarchy: true
}

script:post-response {
  const { etag } = res.getBody();
  etag && bru.setVar('etag', etag);
  
}

settings {
  encodeUrl: true
  timeout: 0
}
